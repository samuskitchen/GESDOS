	<parameter name="codEmpresa" class="java.lang.Long">
		<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
         e.EMPCOD,
         e.EMPNIT,
         d.DEPNOM,
         c.CIUNOM,
         e.EMPCARCON,
         e.EMPNOMCON,
         e.EMPAPE1CON,
         e.EMPCARCON
    FROM EMPRESAS e
    INNER JOIN DEPAR d ON d.DEPCOD = e.DEPCOD
    INNER JOIN CIUDA c ON c.DEPCOD = d.DEPCOD AND c.CIUCOD = e.CIUCOD
    WHERE e.EMPCOD = $P{codEmpresa}]]>
	</queryString>

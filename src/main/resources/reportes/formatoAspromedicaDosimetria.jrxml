	<subDataset name="DatasetAspromedica" uuid="72c64a7a-f3d3-4824-a7e1-de2b2244591e">
		<parameter name="cedulas" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="dosimetros" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="empresas" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString language="plsql">
			<![CDATA[SELECT
	d.DosCod,
          t.TraApe1,
          t.TraApe2,
          t.TraNom,
          t.TraSex,
          c.CarCod,
          t.TraCed,
          t.TraFecIni TraFecIni,
          t.TraFecIni TraFecIni,
          t.TraFecIni TraFecIni,
          d.DosPerRec,
          u.UbiDes,
          r.RadCod,
          e.EstDos,
          e.EmpCod,
          max(e.EstAni),
          max(e.EstMes)
      FROM  dosimetro d
      INNER JOIN traba t ON d.TraCed = t.TraCed
      INNER JOIN cargo c ON d.CarCod = c.CarCod
      INNER JOIN ubicacion u ON d.UbiCod = u.UbiCod
      INNER JOIN radiacion r ON d.RadCod = r.RadCod
      INNER JOIN estudios e ON d.DosCod = e.DosCod AND d.EmpCod = e.EmpCod AND d.TraCed = e.TraCed
      WHERE e.EmpCod IN ($P!{empresas})
      AND   t.TraCed IN ($P!{cedulas})
      AND   d.DosCod IN ($P!{dosimetros})
      GROUP BY d.DosCod, e.EmpCod, t.TraCed, t.TraApe1, t.TraApe2,
               t.TraNom, t.TraSex, c.CarCod, t.TraFecIni, d.DosPerRec,
               u.UbiDes, r.RadCod, e.EstDos]]>
		</queryString>
		<field name="DOSCOD" class="java.math.BigDecimal"/>
		<field name="TRAAPE1" class="java.lang.String"/>
		<field name="TRAAPE2" class="java.lang.String"/>
		<field name="TRANOM" class="java.lang.String"/>
		<field name="TRASEX" class="java.lang.String"/>
		<field name="CARCOD" class="java.lang.String"/>
		<field name="TRACED" class="java.math.BigDecimal"/>
		<field name="TRAFECINI" class="java.sql.Timestamp"/>
		<field name="DOSPERREC" class="java.math.BigDecimal"/>
		<field name="UBIDES" class="java.lang.String"/>
		<field name="RADCOD" class="java.lang.String"/>
		<field name="ESTDOS" class="java.math.BigDecimal"/>
		<field name="EMPCOD" class="java.math.BigDecimal"/>
		<field name="MAX(E.ESTANI)" class="java.math.BigDecimal"/>
		<field name="MAX(E.ESTMES)" class="java.math.BigDecimal"/>
		<group name="pageBreakGroup" isStartNewPage="true">
